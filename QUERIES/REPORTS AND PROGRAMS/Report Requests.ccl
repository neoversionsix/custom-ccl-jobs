SELECT
;UNFINISHED
	R_REPORT_STATUS_DISP = UAR_GET_CODE_DISPLAY(R.REPORT_STATUS_CD)
	, R.SERVER_FULL_NAME
	, R.FILE_NAME
	, R_REPORT_STATUS_DISP = UAR_GET_CODE_DISPLAY(R.REPORT_STATUS_CD)
	, R.TRIGGER_TYPE
	, R.OUTPUT_CONTENT_TYPE_STR
	, R.SENDER_EMAIL

FROM
	CR_REPORT_REQUEST   R
WHERE R.UPDT_DT_TM > CNVTLOOKBEHIND("1,H")
;WHERE R.UPDT_ID = REQINFO ->UPDT_ID
;WHERE R  = = 967510 ;

WITH MAXREC = 100, NOCOUNTER, SEPARATOR=" ", FORMAT, time = 30