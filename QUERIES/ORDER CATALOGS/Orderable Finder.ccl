SELECT
	OC.CATALOG_CD
	, OC.PRIMARY_MNEMONIC
	, CATALOG = UAR_GET_CODE_DISPLAY(OC.CATALOG_TYPE_CD)

FROM
	ORDER_CATALOG   OC

WHERE
    OC.CATALOG_TYPE_CD = 2513
    ; 2513 ; Laboratory
    ; 2516 ; Pharmacy
    ; 2517.00 ; Radiology

    AND CNVTUPPER(OC.PRIMARY_MNEMONIC) = "*TROP*"

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 10