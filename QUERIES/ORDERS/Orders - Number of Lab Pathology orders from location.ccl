SELECT
	COUNT (DISTINCT O.ORDER_ID)


FROM
	ORDERS   O
	, ENCOUNTER   E

PLAN O
	WHERE O.ORIG_ORDER_DT_TM > CNVTLOOKBEHIND("7,D")
		AND O.CATALOG_TYPE_CD = 2513 ; LAB

JOIN E
	WHERE E.ENCNTR_ID = O.ENCNTR_ID
	and e.LOC_FACILITY_CD IN (261889053.00, 261886027.00); Melton Health, Bacchus Marsh

WITH MAXREC = 500, NOCOUNTER, SEPARATOR=" ", FORMAT, time = 10