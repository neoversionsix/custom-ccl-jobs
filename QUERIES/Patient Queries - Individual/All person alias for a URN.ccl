SELECT
	P.PERSON_ID
	, P.NAME_FULL_FORMATTED
	, ALIAS = PA.ALIAS ; EG urn medicare card, etc
	, PA_ALIAS_POOL_DISP = UAR_GET_CODE_DISPLAY(PA.ALIAS_POOL_CD)
	, PA.*

FROM
	PERSON_ALIAS   PA
	, PERSON P
PLAN PA; PERSON_ALIAS
	WHERE
        PA.PERSON_ID = (select x.person_id from person_alias x where x.ALIAS = "1613161") ; URN GOES HERE CHANGE!
    	;AND PA.ACTIVE_IND = 1

JOIN P ; PERSON
	WHERE
		P.PERSON_ID = OUTERJOIN(PA.PERSON_ID)

WITH TIME = 5