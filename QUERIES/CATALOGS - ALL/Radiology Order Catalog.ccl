SELECT
	O_C.CATALOG_CD
	, O_C_CATALOG_DISP = UAR_GET_CODE_DISPLAY(O_C.CATALOG_CD)
	, O_C.CATALOG_TYPE_CD
	, O_C_CATALOG_TYPE_DISP = UAR_GET_CODE_DISPLAY(O_C.CATALOG_TYPE_CD)
	, O_C.ACTIVE_IND
	, O_C.UPDT_DT_TM "YYYY-MM-DD HH:MM:SS"

FROM
	ORDER_CATALOG   O_C

WHERE
    O_C.CATALOG_TYPE_CD = 2517 ; Radiology

ORDER BY O_C.UPDT_DT_TM DESC

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 100