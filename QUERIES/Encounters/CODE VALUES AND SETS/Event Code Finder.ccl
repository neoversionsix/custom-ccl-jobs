SELECT ; EVENT CODE FINDER
	CV1.CODE_SET
	, CV1.CODE_VALUE
	, CV1.DISPLAY
	, CV1.DISPLAY_KEY
	, CV1.END_EFFECTIVE_DT_TM
	, CV1.ACTIVE_IND
	, CV1.DEFINITION
	, CV1.DESCRIPTION

FROM
	CODE_VALUE   CV1

WHERE CODE_SET = 72
AND CV1.DISPLAY = "*4AT*" ; EDIT THIS

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, TIME =10