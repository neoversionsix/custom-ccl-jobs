; uar_get_code_by("MEANING",320,"PROVIDER NUM")
SELECT C.CODE_VALUE
	,C.DISPLAY
	,c.DISPLAY_KEY
	, *
FROM CODE_VALUE C
WHERE
	C.CODE_SET = 320
	and C.CDF_MEANING = "PROVIDER NUM"
with time = 5


; uar_get_code_by("DISPLAYKEY",263,"GPPROVIDER")
SELECT C.CODE_VALUE
	,C.DISPLAY
	,c.DISPLAY_KEY
	, *
FROM CODE_VALUE C
WHERE
	C.CODE_SET = 263
	and C.DISPLAY_KEY = "GPPROVIDER"
with time = 5