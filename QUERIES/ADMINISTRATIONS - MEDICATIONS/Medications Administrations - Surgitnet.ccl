SELECT
	S.ORDER_ID
	, S.SA_MEDICATION_ADMIN_ID
	, SM.ADMIN_DOSAGE
	, S_DOSAGE_UNIT_DISP = UAR_GET_CODE_DISPLAY(S.DOSAGE_UNIT_CD)
	, SM.ADMIN_START_DT_TM "YYYY-MM-DD HH:MM:SS"

FROM
	SA_MEDICATION_ADMIN   S
	, SA_MED_ADMIN_ITEM   SM

PLAN S
 WHERE
 ;S.UPDT_DT_TM > CNVTLOOKBEHIND("1,D")
 ;AND
 S.ORDER_ID = 2153492861 ; Put in an order ID
 and S.ACTIVE_IND = 1

JOIN SM WHERE SM.SA_MEDICATION_ADMIN_ID = S.SA_MEDICATION_ADMIN_ID

WITH NOCOUNTER, SEPARATOR=" ", FORMAT, time = 10