SELECT
	DMONTH = DATETIMETRUNC(CE.UPDT_DT_TM,"MM") "@SHORTDATE"
	, FOR_MONTH = FORMAT (CE.UPDT_DT_TM, "YYYY-MM")
	,CUM_COUNT = COUNT(CE.EVENT_CD) OVER(
			PARTITION BY DATETIMETRUNC(CE.UPDT_DT_TM,"MM")
			)

FROM
	CLINICAL_EVENT   CE

WHERE CE.EVENT_CD = 165086569
	AND CE.UPDT_DT_TM > CNVTLOOKBEHIND("24,M")


ORDER BY
	DMONTH
	, CE.EVENT_CD



WITH NOCOUNTER, SEPARATOR=" ", FORMAT, TIME = 10